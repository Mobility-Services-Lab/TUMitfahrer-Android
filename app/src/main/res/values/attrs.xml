<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontTextView">
        <attr name="font" />
    </declare-styleable>

    <attr name="font" format="enum">
        <enum name="roboto_light" value="1" />
        <enum name="roboto_condensed" value="2" />
        <enum name="roboto_condensed_light" value="3" />
        <enum name="roboto_condensed_bold" value="4" />
        <enum name="roboto_slab" value="5" />
    </attr>

    <declare-styleable name="FloatLabelLayout">
        <attr name="floatLabelTextAppearance" format="reference" />
        <attr name="floatLabelSidePadding" format="reference|dimension" />
    </declare-styleable>

</resources>